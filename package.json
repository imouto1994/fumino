  "scripts": {
    "build": "yarn build-web && yarn post-build && yarn build-admin",
    "build-lite": "yarn build-web-lite && yarn post-build-lite && yarn build-admin",
    "build-admin": "PLATFORM=browser NODE_ENV=production webpack --config ./webpack/admin.prod.config.js",
    "build-web": "PLATFORM=browser NODE_ENV=production webpack --config ./webpack/web.prod.config.js",
    "build-web-lite": "LITE=1 yarn build-web",
    "dev-web": "PLATFORM=browser NODE_ENV=development webpack-dev-server --config ./webpack/web.dev.config.js --progress --color --open",
    "dev-web-lite": "LITE=1 yarn dev-web",
    "dev-admin": "PLATFORM=browser NODE_ENV=development webpack-dev-server --config ./webpack/admin.dev.config.js --progress --color --open",
    "eslint": "eslint ./web --max-warnings 0 --cache --ext .js,.tsx,.ts",
    "lint": "yarn eslint",
    "post-build": "PLATFORM=node node scripts/postBuild",
    "post-build-lite": "LITE=1 yarn post-build",
    "stylelint": "stylelint 'web/**/*.css' 'admin/**/*.css' --cache",
    "tsc": "tsc"
  },
